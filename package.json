{
  "name": "gatsby-source-sanity",
  "description": "Gatsby source plugin for building websites using Sanity.io as a backend.",
  "version": "0.9.9",
  "author": "Sanity.io <hello@sanity.io>",
  "contributors": [
    {
      "name": "Henrique Cavalieri",
      "email": "henrique@kaordica.com.br"
    }
  ],
  "repository": "https://github.com/sanity-io/gatsby-source-sanity",
  "homepage": "https://github.com/sanity-io/gatsby-source-sanity#readme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/gatsby-source-sanity/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "main": "dist/index.js",
  "scripts": {
    "precommit": "tsc",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type-gen": "dts-gen -m @sanity/client"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@sanity/client": "^0.136.2",
    "@sanity/image-url": "^0.136.0",
    "gatsby-source-filesystem": "^2.0.12"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "cross-env": "^5.1.3",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-sanity": "^0.136.0",
    "husky": "^1.2.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  }
}
